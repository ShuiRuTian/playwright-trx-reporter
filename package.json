{
  "name": "playwright-trx-reporter",
  "version": "1.0.7",
  "description": "TRX reporter for playwright",
  "author": {
    "name": "Song Gao"
  },
  "keywords": [
    "playwright",
    "TRX"
  ],
  "license": "ISC",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "test:playwright-single-trx_": "npx playwright install && npx playwright test --config=./playwright.singleTrx.config.ts || exit 0",
    "test:playwright-multi-trx_": "npx playwright install && npx playwright test --config=./playwright.multiTrx.config.ts || exit 0",
    "test:playwright-generate-trx": "npx test:playwright-single-trx && test:playwright-multi-trx",
    "test:jest": "npx jest"
  },
  "devDependencies": {
    "@playwright/test": "^1.30.0",
    "@tsconfig/recommended": "^1.0.2",
    "@types/jest": "^29.4.0",
    "@types/jsdom": "^20.0.1",
    "@types/node": "^16.18.0",
    "@types/uuid": "9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.32.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.27.0",
    "fast-xml-parser": "^4.0.15",
    "jest": "^29.4.1",
    "jsdom": "^21.0.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.5.0",
    "typescript": "^4.9.0"
  },
  "dependencies": {
    "uuid": "^9.0.0"
  }
}